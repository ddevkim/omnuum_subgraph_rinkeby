{
  "name": "om_wallet",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/graphprotocol/om_wallet",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph codegen --output-dir src/types/ && graph build",
    "deploy:testnet": "graph codegen --output-dir src/types/ && graph build && graph deploy --node https://api.studio.thegraph.com/deploy/ om_wallet",
    "deploy:hosting": "graph codegen --output-dir src/types/ && graph build && graph deploy --product hosted-service ddevkim/omnuum-rinkeby",
    "create-local": "graph create --node http://localhost:8020/ om_wallet",
    "remove-local": "graph remove --node http://localhost:8020/ om_wallet",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 om_wallet"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.28.0",
    "@graphprotocol/graph-ts": "0.26.0"
  },
  "devDependencies": {
    "eslint": "^8.11.0",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2"
  }
}
